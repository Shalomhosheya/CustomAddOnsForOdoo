<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>

    <record model="ir.ui.view" id="view_patient_tag_tree">
      <field name="name">patient.tag.tree</field>
      <field name="model">patient.tag</field>
      <field name="arch" type="xml">
        <tree string="Patients Tag" editable="top"> 
          <field name="sequence" widget="handle"/>
          <field name="name"/>
        </tree>
      </field>
    </record>

    <record id="action_hos_patient_tag" model="ir.actions.act_window">
      <field name="name">Patients Tag</field>
      <field name="res_model">patient.tag</field>
      <field name="view_mode">tree</field>
      <field name="help" type="html">
        <p class="o_view_nocontent_create">
          Create a new patient tag
        </p>
      </field>
    </record>

  </data>
</odoo>
